{
	"npmScope": "tradist",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"projects": {
		"auth": {
			"tags": []
		},
		"backend": {
			"tags": []
		},
		"core": {
			"tags": []
		},
		"data": {
			"tags": []
		},
		"frontend": {
			"tags": []
		},
		"frontend-e2e": {
			"tags": [],
			"implicitDependencies": ["frontend"]
		}
	}
}
