{
	"$schema": "https://gist.githubusercontent.com/lkraider/7cedf254bfb39494342d974d7c5a90a7/raw/963c5410d69e894903e57009250d6662dc4943dc/dockerrun-jsen-schema.json",
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "tradist-api-container",
			"image": "${IMAGE_TAG}",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 3000
				}
			]
		}
	],
	"volumes": [],
	"authentication": {
		"bucket": "tradist-docker",
		"key": ".dockercfg"
	}
}
